generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @map("_id")
  elo Int @default(1000)
  profile Json
  password String
  email String @unique
}

